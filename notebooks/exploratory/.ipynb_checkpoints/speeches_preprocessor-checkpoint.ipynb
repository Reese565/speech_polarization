    "def kpm_prefix(pattern):\n",
    "    \"\"\"KPM search prefix array for indices skipping\"\"\"\n",
    "    m = len(pattern)\n",
    "    pt = [0]*m\n",
    "    i = 0\n",
    "    for j in range(m-1):\n",
    "        while i > 0 and pattern[i] != pattern[j+1]:                \n",
    "            i = pt[i-1]\n",
    "        if pattern[i] == pattern[j+1]:\n",
    "            i += 1\n",
    "        pt[j+1] = i        \n",
    "    return pt"
   ]
  },
