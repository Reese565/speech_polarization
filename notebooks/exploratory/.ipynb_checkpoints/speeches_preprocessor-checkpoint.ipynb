    "def recursive_search(text_list, phrases):\n",
    "    l = len(text_list)\n",
    "    if l == 2:\n",
    "        if [text_list[0],text_list[1]] in phrases:\n",
    "            return 'present'\n",
    "        else:\n",
    "            return 'not'\n",
    "    else:\n",
    "        return [recursive_search(text_list[:l//2+1], phrases), recursive_search(text_list[l//2:], phrases)]"
   ]
