    "def kpm_matcher(text,pattern):\n",
    "    \"\"\"KPM matter matcher\"\"\"\n",
    "    match_indeces = []\n",
    "    n = len(text)\n",
    "    m = len(pattern)\n",
    "    pt = kpm_prefix(pattern)\n",
    "    i = 0\n",
    "    for j in range(n-1):\n",
    "        while i > 0 and pattern[i] != text[j+1]:\n",
    "            i = pt[i-1]\n",
    "        if pattern[i] == text[j+1]:\n",
    "            i += 1\n",
    "        if i == m:\n",
    "            match_indeces.append(j+2-m)\n",
    "            i = pt[i-1]\n",
    "    return match_indeces"
   ]
  },
